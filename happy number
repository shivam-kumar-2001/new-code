#include<stdio.h>
int main()
{
	int i = 0,x,n,a=0,b,j,k=0,z;
	printf("enter no.\n");
	scanf("%d",&n);
	if(n==1 || n==7 || n==1111111)
	{
		  printf("happy");
	}
    else if(n>=10)
          {
        	while(n>=10)
        	{
	        k=0;
            	while(n != 0)
                {
            	if(n)
                x= n % 10;
                n /= 10;
                k+=x*x;
                i++;
                }
              if(k==1)
              {
                     printf("happy"); break;
		       }
		   else if(k<10)
              {
             	     printf("not happy"); break;
		      }
           n=k;
       }
   }
   else
     {
     	  printf("not happy");
	 }
	 return z;
}
